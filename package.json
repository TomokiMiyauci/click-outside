{
  "name": "arithmetic4",
  "description": "Type-safe curried arithmetic functions",
  "version": "1.0.3",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.es.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "sideEffects": false,
  "author": {
    "name": "TomokiMiyauci",
    "email": "development.operation6.6.6@gmail.com",
    "url": "https://miyauchi.dev/"
  },
  "homepage": "https://github.com/TomokiMiyauci/arithmetic4#readme",
  "bugs": "https://github.com/TomokiMiyauci/arithmetic4/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/TomokiMiyauci/arithmetic4.git"
  },
  "keywords": [
    "add",
    "subtract",
    "multiply",
    "divide",
    "arithmetic",
    "curry",
    "typescript"
